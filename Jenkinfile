pipeline {
    agent any
    stages {
        stage("stage") {
            steps {
                script {
                    def a = 10
                    def b = 20

                    if (a >= b) {
                        println "Your a value is big, ${a}"
                    } else {
                        println "Your b value is big, ${b}"
                    }
                }
            }
        }
    }
}
