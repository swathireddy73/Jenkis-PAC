pipeline {
    agent any
    stages {
        stage("working with loops") {
            steps {
                script {
                  
                    for (i in 1..10) {
                        println "Your looped value: ${i}"
                    }

                    
                    for (ravi in [10, 20, 30, 40]) {
                        println "Your looped value: ${ravi}"
                    }
                }
            }
        }
    }
}
