pipeline {
    agent any
    stages {
        stage("stage1") {
            steps {
                script {
                    
                    def myfunc(int a, int b) {
result = a+b
                        echo "Hello, welcome to functions concept"
                    }
                    myfunc(10,20)
                    myfunc(100,200)
                }
            }
        }
    }
}
